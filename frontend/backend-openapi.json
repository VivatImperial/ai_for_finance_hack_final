                        "type": "array",
                        "title": "Documents Ids",
                        "default": []
                    }
                },
                "type": "object",
                "required": ["content"],
                "title": "BaseMessage"
            },
            "Body_upload_document_api_v1_document_post": {
                "properties": {
                    "file": {
                        "type": "string",
                        "format": "binary",
                        "title": "File"
                    }
                },
                "type": "object",
                "required": ["file"],
                "title": "Body_upload_document_api_v1_document_post"
            },
            "ChatResponse": {
                "properties": {
                    "chat_id": { "type": "integer", "title": "Chat Id" },
                    "is_active": { "type": "boolean", "title": "Is Active" },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Created At"
                    }
                },
                "type": "object",
                "required": ["chat_id", "is_active", "created_at"],
                "title": "ChatResponse"
            },
            "DocumentResponse": {
                "properties": {
                    "document_id": {
                        "type": "integer",
                        "title": "Document Id"
                    },
                    "minio_url": { "type": "string", "title": "Minio Url" },
                    "filename": { "type": "string", "title": "Filename" },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Created At"
                    },
                    "is_general": { "type": "boolean", "title": "Is General" }
                },
                "type": "object",
                "required": [
                    "document_id",
                    "minio_url",
                    "filename",
                    "created_at",
                    "is_general"
                ],
                "title": "DocumentResponse"
            },
            "ExpandedChatResponse": {
                "properties": {
                    "chat_id": { "type": "integer", "title": "Chat Id" },
                    "is_active": { "type": "boolean", "title": "Is Active" },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Created At"
                    },
                    "prompt": { "type": "string", "title": "Prompt" },
                    "messages": {
                        "items": {
                            "$ref": "#/components/schemas/MessageResponse"
                        },
                        "type": "array",
                        "title": "Messages",
                        "default": []
                    }
                },
                "type": "object",
                "required": ["chat_id", "is_active", "created_at", "prompt"],
                "title": "ExpandedChatResponse"
            },
            "ExpandedDocumentResponse": {
                "properties": {
                    "document_id": {
                        "type": "integer",
                        "title": "Document Id"
                    },
                    "minio_url": { "type": "string", "title": "Minio Url" },
                    "filename": { "type": "string", "title": "Filename" },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Created At"
                    },
                    "is_general": { "type": "boolean", "title": "Is General" },
                    "document_length": {
                        "type": "integer",
                        "title": "Document Length"
                    }
                },
                "type": "object",
                "required": [
                    "document_id",
                    "minio_url",
                    "filename",
                    "created_at",
                    "is_general",
                    "document_length"
                ],
                "title": "ExpandedDocumentResponse"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "MessageResponse": {
                "properties": {
                    "content": { "type": "string", "title": "Content" },
                    "documents_ids": {
                        "items": { "type": "integer" },
                        "type": "array",
                        "title": "Documents Ids"
                    },
                    "message_id": { "type": "integer", "title": "Message Id" },
                    "message_type": {
                        "$ref": "#/components/schemas/MessageType"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Created At"
                    },
                    "hidden_comments": {
                        "anyOf": [{ "type": "string" }, { "type": "null" }],
                        "title": "Hidden Comments"
                    }
                },
                "type": "object",
                "required": [
                    "content",
                    "documents_ids",
                    "message_id",
                    "message_type",
                    "created_at",
                    "hidden_comments"
                ],
                "title": "MessageResponse"
            },
            "MessageType": {
                "type": "integer",
                "enum": [1, 0],
                "title": "MessageType"
            },
            "PromptID": {
                "properties": {
                    "prompt_id": { "type": "integer", "title": "Prompt Id" }
                },
                "type": "object",
                "required": ["prompt_id"],
                "title": "PromptID"
            },
            "Role": { "type": "integer", "enum": [1, 0], "title": "Role" },
            "Token": {
                "properties": {
                    "access_token": {
                        "type": "string",
                        "title": "Access Token"
                    },
                    "token_type": {
                        "type": "string",
                        "title": "Token Type",
                        "default": "bearer"
                    }
                },
                "type": "object",
                "required": ["access_token"],
                "title": "Token"
            },
            "UserCreate": {
                "properties": {
                    "username": { "type": "string", "title": "Username" },
                    "email": { "type": "string", "title": "Email" },
                    "role": {
                        "$ref": "#/components/schemas/Role",
                        "default": 1
                    },
                    "password": { "type": "string", "title": "Password" }
                },
                "type": "object",
                "required": ["username", "email", "password"],
                "title": "UserCreate"
            },
            "UserLogin": {
                "properties": {
                    "email": { "type": "string", "title": "Email" },
                    "password": { "type": "string", "title": "Password" }
                },
                "type": "object",
                "required": ["email", "password"],
                "title": "UserLogin"
            },
            "UserResponse": {
                "properties": {
                    "username": { "type": "string", "title": "Username" },
                    "email": { "type": "string", "title": "Email" },
                    "role": {
                        "$ref": "#/components/schemas/Role",
                        "default": 1
                    },
                    "id": { "type": "integer", "title": "Id" }
                },
                "type": "object",
                "required": ["username", "email", "id"],
                "title": "UserResponse"
            },
            "ValidationError": {
                "properties": {
                    "loc": {
                        "items": {
                            "anyOf": [
                                { "type": "string" },
                                { "type": "integer" }
                            ]
                        },
                        "type": "array",
                        "title": "Location"
                    },
                    "msg": { "type": "string", "title": "Message" },
                    "type": { "type": "string", "title": "Error Type" }
                },
                "type": "object",
                "required": ["loc", "msg", "type"],
                "title": "ValidationError"
            }
        },
        "securitySchemes": {
            "HTTPBearer": { "type": "http", "scheme": "bearer" }
        }
    }
}
